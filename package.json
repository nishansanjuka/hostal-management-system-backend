  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "generate-types": "openapi-typescript openapi.yaml --output src/types/types.ts",
    "build": "npm run generate-types && tsc && npx prisma generate",
